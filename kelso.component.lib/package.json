{
  "name": "@mtnvencenzo/kelso-component-library",
  "private": false,
  "version": "1.0.5",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "loc": "vite",
    "build:prod": "tsc -p tsconfig.prod.json && vite build --mode prod",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook-docs": "storybook build --docs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "pre-ins": "rm -rf node_modules",
    "ins": "yarn install --immutable --immutable-cache --check-cache --network-timeout 100000"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./dist/kelsocl.umd.cjs",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/kelsocl.js",
        "types": "./dist/index.d.ts"
      }
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/kelsocl.umd.cjs",
  "module": "./dist/kelsocl.js",
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.23.0",
    "@chromatic-com/storybook": "3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/argparse": "^2.0.17",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.7",
    "@types/doctrine": "^0.0.9",
    "@types/estree": "^1.0.7",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.15.17",
    "@types/parse-json": "^7.0.0",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-transition-group": "^4.4.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-istanbul": "^3.1.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^0.11.1",
    "jsdom": "26.1.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "storybook": "^8.6.14",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  }
}
