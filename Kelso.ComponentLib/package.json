{
    "name": "kelso-component-library",
    "private": false,
    "version": "1.0.1",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "loc": "vite",
        "build:prod": "tsc -p tsconfig.prod.json && vite build --mode prod",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "build-storybook-docs": "storybook build --docs",
        "test": "vitest",
        "pre-ins": "rm -rf node_modules",
        "ins": "yarn install --immutable --immutable-cache --check-cache --network-timeout 100000"
    },
    "files": [
        "dist/**"
    ],
    "exports": {
        ".": {
            "import": {
                "default": "./dist/kelsocl.umd.cjs",
                "types": "./dist/index.d.ts"
            },
            "require": {
                "default": "./dist/kelsocl.js",
                "types": "./dist/index.d.ts"
            }
        },
        "./dist/style.css": "./dist/style.css"
    },
    "main": "./dist/kelsocl.umd.cjs",
    "module": "./dist/kelsocl.js",
    "types": "./dist/index.d.ts",
    "peerDependencies": {
        "@mui/icons-material": "^6.1.4",
        "@mui/material": "^6.1.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.25.1"
    },
    "devDependencies": {
        "@babel/generator": "^7.23.0",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@fontsource/roboto": "^5.0.8",
        "@mui/icons-material": "^6.1.4",
        "@mui/material": "^6.1.4",
        "@storybook/addon-essentials": "^7.0.26",
        "@storybook/addon-interactions": "^7.0.26",
        "@storybook/addon-links": "^7.0.26",
        "@storybook/blocks": "^7.0.26",
        "@storybook/react": "^7.0.26",
        "@storybook/react-vite": "^7.0.26",
        "@storybook/testing-library": "^0.0.14-next.2",
        "@testing-library/react": "^16.0.0",
        "@types/node": "^20.6.3",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^7.0.1",
        "@vitejs/plugin-react": "^4.3.1",
        "eslint": "^8.56.0",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "eslint-plugin-storybook": "^0.6.12",
        "jsdom": "^24.1.1",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.18.0",
        "storybook": "^7.4.6",
        "typescript": "^5.5.4",
        "vite": "^5.3.5",
        "vite-plugin-css-injected-by-js": "^3.3.0",
        "vite-plugin-dts": "^3.6.0",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^2.0.4"
    }
}
